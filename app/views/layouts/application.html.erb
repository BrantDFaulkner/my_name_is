<!DOCTYPE html>
<html>
<head>
  <title>MyNameIs</title>
  <%= stylesheet_link_tag    'application', media: 'all' %>
  <%= javascript_include_tag 'application' %>
  <%= csrf_meta_tags %>
</head>
<body>

  <nav>
    <% if logged_in? %>
      <%= link_to "SignOut", session_path, method: :delete %>
    <% else %>
      <%= link_to "SignIn", new_session_path %>
      <%= link_to "SignUp", new_user_path %>
    <% end %>
  </nav>

  <hr>

<%= yield %>

</body>
</html>
